{
  "name": "carbon-icons",
  "version": "6.0.2",
  "description": "Icons for the Carbon Design System",
  "main": "dist/carbon-icons.js",
  "module": "dist/carbon-icons.es.js",
  "scripts": {
    "build": "gulp build && gulp copy && gulp build:data",
    "ci-check": "yarn format:diff && yarn test --runInBand",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "dev": "nodemon -e html,svg src/test-server.js",
    "format": "prettier --write \"**/*.{scss,css,js,md,ts}\"",
    "format:diff": "prettier --list-different \"**/*.{scss,css,js,md,ts}\"",
    "prebuild": "gulp clean",
    "semantic-release": "semantic-release",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/carbon-design-system/carbon-icons"
  },
  "keywords": [
    "icons",
    "bluemix",
    "cloud",
    "ibm",
    "carbon"
  ],
  "files": [
    "dist"
  ],
  "maintainers": [
    {
      "name": "Brian Han <@hellobrian>",
      "email": "hellobrianhan@gmail.com"
    }
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "@commitlint/cli": "^6.2.0",
    "@commitlint/config-conventional": "^6.1.3",
    "bluebird": "^3.5.0",
    "chalk": "1.1.3",
    "del": "2.2.0",
    "express": "4.13.4",
    "gh-pages": "0.11.0",
    "gulp": "3.9.0",
    "gulp-dom": "0.9.0",
    "gulp-edit-xml": "^2.0.0",
    "gulp-rename": "1.2.2",
    "gulp-svg-sprite": "1.2.18",
    "gulp-svgo": "1.0.3",
    "husky": "^0.14.3",
    "jest": "16.0.2",
    "lint-staged": "^7.1.0",
    "nodemon": "1.9.2",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.12.1",
    "remove-svg-properties": "0.3.2",
    "semantic-release": "^6.3.6",
    "smart-svg-path": "^1.0.2",
    "svg-points": "^5.0.0",
    "svgxuse": "1.1.16",
    "validate-commit-msg": "^2.12.1",
    "xml2js": "0.4.17"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{scss,css,js,md}": [
      "yarn format",
      "git add"
    ]
  },
  "prettier": {
    "jsxBracketSameLine": true,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
